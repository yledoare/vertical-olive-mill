<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">


<record id="olive_arrival_seq" model="ir.sequence">
    <field name="name">Olive Arrival</field>
    <field name="code">olive.arrival</field>
    <field name="prefix">BE-%(year)s-</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="use_date_range" eval="True"/>
    <field name="company_id" eval="False"/>
</record>

<record id="olive_oil_production_seq" model="ir.sequence">
    <field name="name">Olive Oil Production</field>
    <field name="code">olive.oil.production</field>
    <field name="prefix">P-%(year)s-</field>
    <field name="padding">3</field>
    <field name="number_next">1</field>
    <field name="use_date_range" eval="True"/>
    <field name="company_id" eval="False"/>
</record>

<record id="olive_oil_merge_lot_seq" model="ir.sequence">
    <field name="name">Olive Oil Merge Lot</field>
    <field name="code">olive.oil.merge.lot</field>
    <field name="prefix">FUSION</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="company_id" eval="False"/>
</record>

<record id="olive_oil_analysis_seq" model="ir.sequence">
    <field name="name">Olive Oil Analysis</field>
    <field name="code">olive.oil.analysis</field>
    <field name="prefix">AN-%(year)s-</field>
    <field name="padding">3</field>
    <field name="number_next">1</field>
    <field name="company_id" eval="False"/>
</record>


</odoo>
