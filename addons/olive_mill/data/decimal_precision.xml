<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">


<record forcecreate="True" id="olive_parcel_area" model="decimal.precision">
    <field name="name">Olive Parcel Area</field>
    <field name="digits">4</field>
</record>

<record forcecreate="True" id="olive_weight" model="decimal.precision">
    <field name="name">Olive Weight</field>  <!-- Also used for Olive Oil at the moment -->
    <field name="digits">2</field>
</record>

<record forcecreate="True" id="olive_oil_volume" model="decimal.precision">
    <field name="name">Olive Oil Volume</field>
    <field name="digits">2</field>
</record>

<record forcecreate="True" id="olive_oil_ratio" model="decimal.precision">
    <field name="name">Olive Oil Ratio</field>
    <field name="digits">2</field>
</record>

<record forcecreate="True" id="olive_oil_density" model="decimal.precision">
    <field name="name">Olive Oil Density</field>
    <field name="digits">3</field>
</record>

